@model NoteTitle
@{
    ViewData["Title"] = "UpdateNote";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<form asp-controller="Title" asp-action="UpdateNote">
    @Html.HiddenFor(x=>x.TitleId)
    <div class="form-group">
        <label for="exampleFormControlSelect1">Başlık Seç</label>
        @Html.TextBoxFor(x=>x.TitleName, new {@class="form-control"})
        <br />
    </div>
    <button class="btn btn-dark">Ekle</button>
</form>


